{
  "vetur.trace.server": "verbose",
  "vetur.useWorkspaceDependencies": true,
  "vetur.completion.autoImport": true,
  "vetur.completion.useScaffoldSnippets": true,
  "vetur.completion.tagCasing": "kebab",
  "vetur.validation.template": true,
  "vetur.validation.style": true,
  "vetur.format.enable": true,
  "emmet.showAbbreviationSuggestions": true,
  "vetur.format.defaultFormatter.js": "vscode-typescript",
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "vue", "json"],
  "emmet.includeLanguages": { "vue-html": "html", "jsx": "javascript-react" },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.showExpandedAbbreviation": "always",
  "snippets.loadFromExtensions": true
}
